<% provide(:title, @movie.name) %>
<div class="row">
  <aside class="span4">
    <section>
      <h1>
        <%= @movie.name %>
      </h1>
      <h3>
        <%= link_to "Youtube Link", @movie.url %>
      </h3>
    </section>
    <section>
      <%= render 'shared/movie_stats' %>
    </section>
    <section>
      <%= render 'join_form' if signed_in? %>
    </section>
  </aside>
  <div class="span8">

  </div>
</div>