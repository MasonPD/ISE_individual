<% provide(:title, 'New Movies') %>
<h1>new movies</h1>

<div class="row">
  <div class="span6 offset3">
    <%= form_for( @movie ) do |f| %>
        <%= render 'shared/error_messages', object: f.object %>
        <%= f.label :name %>
        <%= f.text_field :name %>

        <%= f.label :url %>
        <%= f.text_field :url %>

        <%= f.submit "Let's do it!", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>